<!DOCTYPE html>
<html>
  <head>
    <title>Luz Elena Castro</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <!-- Basic stylesheet -->
  </head>

  <body>
    <div id="wrapper" class="toggled">
      <!-- Sidebar -->
      <!-- /#sidebar-wrapper -->
      <div id="sidebar-wrapper">
        <ul class="sidebar-nav">
          <li class="sidebar-brand">
            Luz Castro
          </li>
          <li>  <%= link_to 'Home', '/home'%>
          </li>
          <li><%= link_to 'About Me', '/aboutme' %>
          </li>
          <li>  <%= link_to 'Exhibitions & Awards', '/exhibitions'%>
          </li>
          <li>  <%= link_to 'Photojournalism', '/photojournalism'%>
          </li>
          <li>  <%= link_to 'Interviews & Features', '/features'%>
          </li>
          <li>  <%= link_to 'Blog', '/blog'%>
          </li>
        </ul>
      </div>
      <!-- Page Content -->
      <div id="page-content-wrapper">
      <div class="container-fluid">
          <div class="row">
            <div class="col-lg-12">
              <div class="jumbotron jumbotron-fluid">
                <a href="" class="btn btn-default" id="menu-toggle">
                  <button type="button" class="btn btn-default btn-lg">
                    <i class="fa fa-bars" aria-hidden="true"></i>
                  </button>
                </a>
                  <h1 class="clip-text clip-text_one">
                    LUZ ELENA CASTRO
                  </h1>
                  <div class ="clip-p">PHOTOJOURNALIST</div>
              </div>
              <%= yield %>
            </div>
          </div>
        </div>
      </div>
      <!-- /#page-content-wrapper -->
    </div>
    <!-- /#wrapper -->
  </body>
</html>
