<!DOCTYPE html>
<html>
  <head>
    <title>Luz Elena Castro</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <!-- Basic stylesheet -->
  </head>

  <body>
    <div id="wrapper">
      <!-- Sidebar -->
      <!-- /#sidebar-wrapper -->
      <div id="sidebar-wrapper">
        <ul class="sidebar-nav">

          <li class="sidebar-brand">
             <%= link_to 'Luz Castro', root_path %>
          </li>

        <li>

          <li>  <%= link_to 'Home', '/'%>
          </li>
          <li><%= link_to 'About Me', '/aboutme' %>
          </li>
          <li>  <%= link_to 'Exhibitions & Awards', '/exhibitions'%>
          </li>
          <li>  <%= link_to 'Photojournalism', '/photojournalism'%>
          </li>
          <li>  <%= link_to 'Interviews & Features', '/features'%>
          </li>
          <li>  <%= link_to 'Blog', '/blog'%>
          </li>

        </ul>
      </div>
      <!-- Page Content -->
      <div id="page-content-wrapper">
        <div class="container-fluid">
          <div class="row" id="header">
            <a href="" class="btn btn-default" id="menu-toggle">
              <%= image_tag("menu.png", :alt => "menu", :class => "menu") %>
            </a>
            <div class="clip-text clip-text_one">LUZ ELENA CASTRO</div>
            <div class ="clip-p">PHOTOJOURNALIST</div>
          </div>
          <div class="row">
            <div class="col-lg-12">
              <%= yield %>
            </div>
          </div>
        </div>
        </div>
      <!-- /#page-content-wrapper -->
    </div>
    <!-- /#wrapper -->
  </body>
</html>
